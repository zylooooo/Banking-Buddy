server.port=8083
spring.application.name=ai-service

# OpenAI Configuration
openai.api.key=${OPENAI_API_KEY:}
openai.api.url=${OPENAI_API_URL:https://api.openai.com/v1}

# Service URLs
services.user-service.url=${USER_SERVICE_URL:http://localhost:8080}
services.client-service.url=${CLIENT_SERVICE_URL:http://localhost:8081}
services.transaction-service.url=${TRANSACTION_SERVICE_URL:http://localhost:8082}

# Logging
logging.level.com.BankingBuddy.ai_service=INFO